<!DOCTYPE html>
<html>
  <head>
    <% include ./partials/head %>
  </head>

  <body class="home">
   <header><% include ./partials/header %></header>
    
    <div class="container">
     <div class="jumbotron text-center">
            <h1>Top Terms</h1>
    </div>
    
     <% terms.forEach(function(query, index){ %>
       <div class="col-sm-6 panel">
         <a href="/results?q=<%= query.term %>" class="btn btn-block btn-lg btn-<%=query.style %>"><%= query.term %></a>
       </div>
     
     <%}) %>
      
    </div>
 
    <footer>
      <% include ./partials/footer %>
    </footer>
    
  </body>
</html>